package util;

import java.util.ArrayList;

public class Emprestimo {
	public int indiceLivro;
	public String aluno;

	public Emprestimo(int indiceLivro, String aluno) {
		this.indiceLivro = indiceLivro;
		this.aluno = aluno;
	}

	public static void listarEmprestimos() {
		System.out.println("Empr√©stimos realizados:");
		for (Emprestimo e : emprestimos) {
			System.out.println("Aluno: " + e.aluno + " | Livro: " + livros.get(e.indiceLivro).titulo);
		}
	}
}